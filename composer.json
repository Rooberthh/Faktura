{
    "name": "rooberthh/faktura",
    "description": "A invoice package for laravel.",
    "type": "library",
    "keywords": [
        "php",
        "package"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roberth Evaldsson",
            "email": "evaldsson.roberth@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/rooberthh/faktura/issues"
    },
    "funding": [],
    "require": {
        "php": "^8.3",
        "illuminate/config": "^12.0",
        "illuminate/contracts": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/events": "^12.0",
        "moneyphp/money": "^4.7"
    },
    "require-dev": {
        "fakerphp/faker": "^1.24",
        "laravel/pint": "^1.13.7",
        "orchestra/testbench": "^10.6",
        "pestphp/pest": "^4.1",
        "phpstan/phpstan": "^2.1"
    },
    "suggest": {},
    "autoload": {
        "psr-4": {
            "Rooberthh\\Faktura\\": "src/",
            "Rooberthh\\Faktura\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rooberthh\\Faktura\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        },
        "optimize-autoloader": true,
        "sort-packages": true,
        "classmap-authoritative": true
    },
    "scripts": {
        "pint": [
            "./vendor/bin/pint"
        ],
        "stan": [
            "./vendor/bin/phpstan analyse --memory-limit=3g"
        ]
    },
    "scripts-descriptions": {
        "pint": "Run Laravel Pint on the codebase.",
        "stan": "Run PhpStan on the codebase."
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rooberthh\\Faktura\\FakturaServiceProvider"
            ]
        }
    }
}

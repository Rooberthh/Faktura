{
    "name": "rooberthh/faktura",
    "description": "A invoice package for laravel.",
    "type": "library",
    "keywords": [
        "php",
        "package"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roberth Evaldsson",
            "email": "evaldsson.roberth@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/rooberthh/faktura/issues"
    },
    "funding": [],
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "laravel/pint": "^1.13.7",
        "pestphp/pest": "^3.5",
        "phpstan/phpstan": "^1.10.50"
    },
    "suggest": {},
    "autoload": {
        "psr-4": {
            "Rooberthh\\Faktura\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rooberthh\\Faktura\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "pestphp/pest-plugin": true
        },
        "optimize-autoloader": true,
        "sort-packages": true,
        "classmap-authoritative": true
    },
    "scripts": {
        "pint": [
            "./vendor/bin/pint"
        ],
        "stan": [
            "./vendor/bin/phpstan analyse --memory-limit=3g"
        ]
    },
    "scripts-descriptions": {
        "pint": "Run Laravel Pint on the codebase.",
        "stan": "Run PhpStan on the codebase."
    }
}
